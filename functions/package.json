{
  "name": "airtable-api-proxy",
  "version": "1.0.0",
  "description": "Api proxy for connecting public requests with airtable via private requests",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enspiral/airtable-api-proxy.git"
  },
  "keywords": [
    "airtable",
    "api",
    "proxy"
  ],
  "author": "agentlewis",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/enspiral/airtable-api-proxy/issues"
  },
  "homepage": "https://github.com/enspiral/airtable-api-proxy#readme",
  "scripts": {
    "burnthemall": "rm -rf package-lock.json node_modules; npm i",
    "lint": "eslint .",
    "firebase": "./node_modules/.bin/firebase",
    "serve": "./node_modules/.bin/firebase serve --only functions",
    "shell": "./node_modules/.bin/firebase functions:shell",
    "deploy": "./node_modules/.bin/firebase deploy --only functions",
    "logs": "./node_modules/.bin/firebase functions:log",
    "start": "npm run shell"
  },
  "dependencies": {
    "airtable": "^0.5.3",
    "cors": "^2.8.4",
    "firebase-admin": "~5.12.0",
    "firebase-functions": "^1.0.2",
    "ramda": "0.25.0",
    "ramda-adjunct": "^2.7.0"
  },
  "devDependencies": {
    "eslint": "^4.12.0",
    "eslint-plugin-promise": "^3.6.0",
    "firebase-tools": "^3.18.4"
  },
  "private": true
}
